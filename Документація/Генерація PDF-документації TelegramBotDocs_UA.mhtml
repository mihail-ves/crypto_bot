async def signal_summary_command(update: Update, context: ContextTypes.DEFA=
ULT_TYPE):
    if len(price_history) &lt; 20:
        await update.message.reply_text("=D0=9D=D0=B5=D0=B4=D0=BE=D1=81=D1=
=82=D0=B0=D1=82=D0=BD=D1=8C=D0=BE =D1=96=D1=81=D1=82=D0=BE=D1=80=D1=96=D1=
=97 =D1=86=D1=96=D0=BD =D0=B4=D0=BB=D1=8F =D0=B0=D0=BD=D0=B0=D0=BB=D1=96=D0=
=B7=D1=83.")
        return

    df =3D pd.DataFrame(price_history, columns=3D["price"])
    delta =3D df["price"].diff()
    gain =3D delta.clip(lower=3D0)
    loss =3D -delta.clip(upper=3D0)
    avg_gain =3D gain.rolling(window=3D14).mean()
    avg_loss =3D loss.rolling(window=3D14).mean()
    rs =3D avg_gain / avg_loss
    rsi =3D 100 - (100 / (1 + rs))
    latest_rsi =3D round(rsi.iloc[-1], 2)

    df["tr"] =3D df["price"].diff().abs()
    atr =3D round(df["tr"].rolling(window=3D14).mean().iloc[-1], 2)
    breakout =3D df["price"].iloc[-1] &gt; df["price"].rolling(window=3D20)=
.max().iloc[-2]

