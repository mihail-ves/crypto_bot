def detect_signal(prices: list) -&gt; str:
    """
    =D0=90=D0=BD=D0=B0=D0=BB=D1=96=D0=B7=D1=83=D1=94 RSI, ATR =D1=96 Breako=
ut =D0=B4=D0=BB=D1=8F =D0=BF=D1=80=D0=B8=D0=B9=D0=BD=D1=8F=D1=82=D1=82=D1=
=8F =D1=80=D1=96=D1=88=D0=B5=D0=BD=D0=BD=D1=8F BUY/SELL.
    """
    if len(prices) &lt; 20:
        return "NO_SIGNAL"

    df =3D pd.DataFrame(prices, columns=3D["price"])
    df["returns"] =3D df["price"].pct_change()

    # RSI
    delta =3D df["price"].diff()
    gain =3D delta.clip(lower=3D0)
    loss =3D -delta.clip(upper=3D0)
    avg_gain =3D gain.rolling(window=3D14).mean()
    avg_loss =3D loss.rolling(window=3D14).mean()
    rs =3D avg_gain / avg_loss
    rsi =3D 100 - (100 / (1 + rs))
    latest_rsi =3D rsi.iloc[-1]

    # ATR
    df["high"] =3D df["price"]
    df["low"] =3D df["price"]
    df["close"] =3D df["price"]
    df["tr"] =3D df["high"] - df["low"]
    atr =3D df["tr"].rolling(window=3D14).mean().iloc[-1]

    # Breakout
    breakout =3D df["price"].iloc[-1] &gt; df["price"].rolling(window=3D20)=
.max().iloc[-2]

    # =D0=9B=D0=BE=D0=B3=D1=96=D0=BA=D0=B0 =D1=81=D0=B8=D0=B3=D0=BD=D0=B0=
=D0=BB=D1=83
    if breakout and latest_rsi &lt; 70:
        return "BUY"
    elif latest_rsi &gt; 80:
        return "SELL"
    return "NO_SIGNAL"

